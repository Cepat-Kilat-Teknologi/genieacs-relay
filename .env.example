# GenieACS API Gateway Configuration
# Copy this file to .env and fill in your actual values

# GenieACS server URL
GENIEACS_BASE_URL=http://localhost:7557

# NBI Authentication Key (REQUIRED - must be set for API authentication)
NBI_AUTH_KEY=your-secret-key-here

# Server address (optional, defaults to :8080)
# SERVER_ADDR=:8080